# Flask Configuration
FLASK_APP=src/main.py
FLASK_DEBUG=1 # Set to 0 in production
FLASK_ENV="your_environment_here" # Use: development
PORT="port_number_here" # Use: 5000

# Security (Placeholder values)
SECRET_KEY="your_super_secret_key_here"
JWT_SECRET_KEY="your_jwt_secret_here"

# Database Configuration
# PostgreSQL database from docker-compose
DB_HOST="your_db_host_here" # Use: localhost
DB_PORT="your_db_port_here" # Use: 5432
DB_USER="your_db_username" # Use: soft_eng_user
DB_PASSWORD="your_db_password" # Use: dev_password_2026
DB_NAME="your_database_name" # Use: soft_eng_db
DATABASE_URL=postgresql://user:password@localhost:5432/dbname # Use: postgresql://soft_eng_user:dev_password_2026@localhost:5432/soft_eng_db
